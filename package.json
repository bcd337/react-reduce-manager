{
  "name": "react-reduce-manager",
  "version": "0.1.7",
  "license": "MIT",
  "description": "Hook for React useReducer",
  "repository": "github:bcd337/react-reduce-manager",
  "homepage": "https://github.com/bcd337/react-reduce-manager",
  "keywords": [
    "react-reduce-manager",
    "useReducer"
  ],
  "author": "Muhamad Iqbal",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src/*.ts --ignore src/types.ts --extensions .ts --out-dir lib ",
    "build:es": "babel src/*.ts --ignore src/types.ts --extensions .ts --out-dir es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/react-reduce-manager.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/react-reduce-manager.min.js",
    "build:types": "tsc",
    "build": "rimraf dist lib es && npm run build:types && npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "api-types": "api-extractor run --local",
    "clean": "rimraf lib dist es",
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "pretest": "tsc --noEmit",
    "test": "jest",
    "test:ci": "jest",
    "test:dev": "jest --watch --no-coverage",
    "test:coverage:watch": "jest --watch"
  },
  "engines": {
    "node": ">=12.7.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@babel/plugin-transform-typescript": "^7.18.8",
    "@babel/preset-env": "^7.18.9",
    "@microsoft/api-extractor": "^7.28.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@swc/core": "^1.2.219",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "concurrently": "^7.3.0",
    "esbuild": "^0.14.50",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "shx": "^0.3.4",
    "typescript": "^4.7",
    "cross-env": "^7.0.3"
  }
}
